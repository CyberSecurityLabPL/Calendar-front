{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrderSeparation": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^[*/]$",
    "^@ui/(.*)$",
    "^[./]"
  ],
  "bracketSpacing": true,
  "bracketSameLine": true,
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "requirePragma": false,
  "arrowParens": "avoid",
  "endOfLine": "lf"
}